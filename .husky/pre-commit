npx husky add .husky/pre-commit "yarn lint-staged"
chmod +x .husky/pre-commit  # Đảm bảo file có quyền thực thi

yarn test:unit || (
    echo 'Your code need to be tested...................... !';
    false;
)

yarn format || (
    echo 'Your code need to be formated...................... !';
    false;
)

yarn lint || (
    echo 'error rulee...................... !';
    false;
)

yarn build || (
    echo 'Build errorr...................... !';
    false;
)